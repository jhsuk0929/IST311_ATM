<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.WithdrawController">
   <children>
      <Label layoutX="307.0" layoutY="51.0" text="WITHDRAW">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="264.0" layoutY="92.0" text="ENTER AN AMOUNT TO WITHDRAW">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Button fx:id="withdrawButton" defaultButton="true" layoutX="292.0" layoutY="342.0" mnemonicParsing="false" onAction="#handleButtonClick" prefHeight="43.0" prefWidth="138.0" style="-fx-background-color: #47b0ed;" text="WITHDRAW" />
      <Button fx:id="cancelAmountButton" cancelButton="true" layoutX="291.0" layoutY="402.0" mnemonicParsing="false" onAction="#handleButtonClick" prefHeight="43.0" prefWidth="138.0" style="-fx-background-color: #e8594f;" text="CANCEL" />
      <TextField fx:id="amountTextField" layoutX="282.0" layoutY="185.0" prefHeight="37.0" prefWidth="178.0" promptText="ENTER AN AMOUNT">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <Label layoutX="263.0" layoutY="189.0" text="\$">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="1.0" layoutY="275.0" opacity="0.0" prefHeight="20.0" prefWidth="720.0" text="INSUFFICIENT FUNDS AVAILABLE" textAlignment="CENTER" textFill="#e8594f">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Label>
      <Label layoutX="194.0" layoutY="232.0" text="ATM WILL NOT DISPENSE CHANGE. ENTER A POSITIVE WHOLE DOLLAR AMOUNT." textFill="#545454">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
